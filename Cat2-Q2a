//c structure 
#include<string.h>
#include<stdio.h>
struct employee {
     char name [25];
     int ID;
     char department[20];
     float salary;
     char email [50];
     };
 struct employee employee1;
 int main(){
     struct employee employee1;
     printf("enter name");
     scanf("%s", & employee1.name);
     printf("enter ID");
     scanf("%d", & employee1.ID);
     printf("enter department");
     scanf("%s", & employee1.department);
     printf("enter salary");
     scanf("%f", & employee1.salary);
     printf("enter email");
     scanf("%s", & employee1.email);
     
     return 0;
     }
